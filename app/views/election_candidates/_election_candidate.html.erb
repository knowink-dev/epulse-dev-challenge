<div id="<%= dom_id election_candidate %>">
  <% if action_name != "show" %>
    <%= link_to "Show this election candidate", election_candidate, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this election candidate', edit_election_candidate_path(election_candidate), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
