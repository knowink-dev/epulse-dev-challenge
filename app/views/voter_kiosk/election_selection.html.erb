<div class="container">
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
      <div class="text-center">
        <p>Hello, <strong class="text-info"><%= @voter.full_name %></strong></p>
        <h5>Lets find the elections in your area!</h5>
      </div>
      <hr>
      <% @elections.each do |election_selection| %>
        <p><span><strong class="text-info"><%= election_selection.election_title %></strong> | <strong>Election Date:</strong> <%= date_formatter election_selection.election_date %> <%= link_to 'Select', vote_for_election_candidate_path(election_id: election_selection.id, voter_id: @voter.id) %></span></p>
      <% end %>
    </div>
    <div class="col-md-3"></div>
  </div>
</div>
