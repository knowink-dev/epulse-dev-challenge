<div class="flex flex-wrap">
  <div class="w-full">
    <nav class="relative flex flex-wrap items-center justify-between px-2 py-3 bg-gray-500">
      <div class="container px-4 mx-auto flex flex-wrap items-center justify-between">
        <div class="w-full relative flex justify-between lg:w-auto px-3 lg:static lg:block lg:justify-start">
          <a class="text-sm font-bold leading-relaxed inline-block mr-4 py-2 whitespace-nowrap uppercase text-white" href="#pablo">
            <i class="fas fa-vote-yea text-info"></i> <%= fa_icon("chevron-right", class: 'text-info') %> Electron
          </a>
          <button class="cursor-pointer text-xl leading-none px-3 py-1 border border-solid border-transparent rounded bg-transparent block lg:hidden outline-none focus:outline-none" type="button">
            <span class="block relative w-6 h-px rounded-sm bg-white"></span>
            <span class="block relative w-6 h-px rounded-sm bg-white mt-1"></span>
            <span class="block relative w-6 h-px rounded-sm bg-white mt-1"></span>
          </button>
        </div>
        <div class="flex lg:flex-grow items-center" id="example-navbar-info">
          <ul class="flex flex-col lg:flex-row list-none ml-auto">
            <li class="nav-item">
              <%= link_to 'Precinct Management', precincts_path, class: 'px-3 py-2 flex items-center text-xs uppercase font-bold leading-snug text-white hover:opacity-75' %>
            </li>
            <li class="nav-item">
              <%= link_to 'Elections', elections_path, class: 'px-3 py-2 flex items-center text-xs uppercase font-bold leading-snug text-white hover:opacity-75' %>
            </li>
            <li class="nav-item">
              <%= link_to 'Pollsters', poll_workers_path, class: 'px-3 py-2 flex items-center text-xs uppercase font-bold leading-snug text-white hover:opacity-75' %>
            </li>
            <li class="nav-item">
              <%= link_to 'Voter Registry', voters_path, class: 'px-3 py-2 flex items-center text-xs uppercase font-bold leading-snug text-white hover:opacity-75' %>
            </li>
            <li class="nav-item">
              <%= link_to voter_search_path, class: 'px-3 py-2 flex items-center text-xs uppercase font-bold leading-snug text-white hover:opacity-75' do %>
                <%= fa_icon("user-circle", class: 'text-info') %> <span class="ml-2">Emulate Voter</span>
              <% end %>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </div>
</div>